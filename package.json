{
  "name": "timbr",
  "version": "1.0.0",
  "description": "A plant reminder and database for amateur botanists",
  "private": true,
  "main": "server.js",
  "scripts": {
    "build-dev": "npm run clean; webpack  --inline --progress --color --config config/webpack.dev.config.js",
    "build": "npm run clean; webpack  --inline --progress --color --config config/webpack.prod.config.js",
    "start-dev": "npm run build-dev; node server.js",
    "start": "npm run build; node server.js",
    "clean": "rm dist/*",
    "lint": "eslint src/**/*.js",
    "lint-fix": "eslint src/**/*.js --fix",
    "test-b": "mocha test/backend",
    "test-bu": "mocha test/backend/unit",
    "test-bi": "mocha test/backend/integration --timeout 60000",
    "test-f": "mocha test/frontend",
    "test": "npm run test-b; npm run test-f;"
  },
  "license": "MIT",
  "_moduleAliases": {
    "@": "./",
    "@b": "./src/backend",
    "@f": "./src/frontend",
    "@t": "./test"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "faker": "^5.1.0",
    "html-webpack-plugin": "^4.4.1",
    "mocha": "^8.1.3",
    "node-sass": "^4.14.1",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sass-loader": "^10.0.2",
    "sinon": "^9.0.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.4"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.7",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "chalk": "^4.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^7.21.0",
    "history": "^5.0.0",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "pg": "^8.3.3",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5"
  }
}
